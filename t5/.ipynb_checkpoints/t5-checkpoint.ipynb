   "source": [
    "import scipy.stats as st\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "metadata": {},
   "outputs": [],
   "source": [
    "theta = 10\n",
    "N = 100"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 37,
   "metadata": {},
   "outputs": [],
   "source": [
    "sample = st.pareto.rvs(b=theta - 1, size=N)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 38,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "10.18835039673579\n"
     ]
    }
   ],
   "source": [
    "C = np.sum(np.log(sample))\n",
    "\n",
    "theta1 = N / C + 1\n",
    "\n",
    "print(theta1)"
   ]
  },
